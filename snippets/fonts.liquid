{% # theme-check-disable %}
{%- unless settings.type_body_font.system? -%}
  <link
    rel="preload"
    as="font"
    href="{{ settings.type_body_font | font_url }}"
    type="font/woff2"
    crossorigin
    fetchpriority="low"
  >
{%- endunless -%}
{%- unless settings.type_subheading_font.system? -%}
  <link
    rel="preload"
    as="font"
    href="{{ settings.type_subheading_font | font_url }}"
    type="font/woff2"
    crossorigin
    fetchpriority="low"
  >
{%- endunless -%}
{%- unless settings.type_heading_font.system? -%}
  <link
    rel="preload"
    as="font"
    href="{{ settings.type_heading_font | font_url }}"
    type="font/woff2"
    crossorigin
    fetchpriority="low"
  >
{%- endunless -%}
{%- unless settings.type_accent_font.system? -%}
  <link
    rel="preload"
    as="font"
    href="{{ settings.type_accent_font | font_url }}"
    type="font/woff2"
    crossorigin
    fetchpriority="low"
  >
{%- endunless -%}

{% style %}
    @font-face {
        font-family: 'Articulat CF';
        src: url('{{ "ArticulatCF-Regular.woff2" | asset_url }}') format('woff2'),
        url('{{ "ArticulatCF-Regular.woff" | asset_url }}') format('woff'),
        url('{{ "ArticulatCF-Regular.ttf" | asset_url }}') format('ttf');;
        font-weight: 400;
        font-style: normal;
        font-display: swap;
    }

    @font-face {
        font-family: 'Articulat CF';
        src: url('{{ "ArticulatCF-Medium.woff2" | asset_url }}') format('woff2'),
        url('{{ "ArticulatCF-Medium.woff" | asset_url }}') format('woff'),
        url('{{ "ArticulatCF-Medium.ttf" | asset_url }}') format('ttf');
        font-weight: 500;
        font-style: normal;
        font-display: swap;
    }
{% endstyle %}
{% # theme-check-enable %}
